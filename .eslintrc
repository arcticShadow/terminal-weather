{
    "extends": [
      // "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    
    
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021,
    },
  
    "env": {
      "node": true
    }
  }
  